{
    "env": {
        "node": true,
        "commonjs": true,
        "es2021": true
    },
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "quotes": [ 2, "backtick" ],
        "indent": [ 2, 2, { "SwitchCase": 1 } ],
        "key-spacing": 2,
        "keyword-spacing": 2,
        "no-console": 1,
        "no-unused-vars": [ 2, { "argsIgnorePattern": "^_" } ]
    }
}
